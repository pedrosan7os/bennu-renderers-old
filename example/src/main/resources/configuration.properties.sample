db.alias=//localhost:3306/dot?useUnicode=true&amp;characterEncoding=UTF-8&amp;clobCharacterEncoding=UTF-8&amp;zeroDateTimeBehavior=convertToNull
db.user=root
db.pass=

#for extra goodies, turn this on (only in development mode)
